<Page x:Class="SVGReader.ImageTab"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
          xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
      xmlns:local="clr-namespace:SVGReader"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ImageTab" Background="Transparent">
    <Grid>
        <Border CornerRadius="5" BorderThickness="0" Margin="20" Grid.Column="0" Grid.Row="0" Background="DarkGray">
            <svgc:SvgViewbox x:Name="SVGImage"/>
        </Border>
    </Grid>
</Page>
